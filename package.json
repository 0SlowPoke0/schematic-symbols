{
  "name": "schematic-symbols",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "start": "bun run build && bun --hot run dev-server.ts",
    "build": "rm symbols/index.ts && bunx make-vfs ./symbols --content-format import-default --outfile ./symbols/index.ts",
    "generate": "bun run scripts/generate-symbols-from-asset-svgs.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/svg-path-parser": "^1.1.6",
    "make-vfs": "1.0.10",
    "svg-path-parser": "^1.1.0",
    "svgson": "^5.3.1",
    "transformation-matrix": "^2.16.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {}
}
